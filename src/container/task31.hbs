    <section>
      {{#with navigation}}
      <div class="row">
        {{#each links}}
        <div class="col-2">
          <a href={{href}}>{{text}}</a>
        </div>
        {{/each}}
        <div class="col-2">
          {{#with button}}
          <a class="btn btn-primary" href={{href}}>{{text}}</a>
          {{/with}}
        </div>
      </div>
      {{/with}}
      <hr>
    </section>

    {{!-- <section>

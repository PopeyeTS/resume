    <section>
      {{#with header}}
      <h1>{{title}}</h1>
      <p>{{description}}</p>
      <div>
        {{#with button}}
        <a class="btn btn-primary" href={{link}}>{{text}}</a>
        {{/with}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with city}}
      <h2>{{title}}</h2>
      <div class="row">
        {{#each cards}}
        <div class="col-5">
          <div class="card">
            <img class="card-img-top" src={{picture.src}} alt={{picture.alt}}>
            <div class="card-body">
              <div class="card-title">{{title}}</div>
              <p>{{description}}</p>
              <a class="btn btn-primary" href={{button.href}}>{{button.text}}</a>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with winner}}
      <h2>{{title}}</h2>
      {{#with table}}
      <table class="table">
        <thead>
          <tr>
            {{#each head}}
            <th> {{this}} </th>
            {{/each}}
          </tr>
        </thead>
        <tbody>
          {{#each user}}
          <tr>
            <td>{{place}}</td>
            <td>{{name}}</td>
            <td>{{surname}}</td>
            <td>{{nickname}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      {{/with}}
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with gallery}}
      <h2>{{title}}</h2>
      <div class="row">
        {{#each cards}}
        <div class="col-4">
          <div class="card">
            <div class="card-header">
              <div>{{name}}</div>
            </div>
            <ul class="list-group">
              {{#each items}}
              <li class="list-group-item"> {{this}} </li>
              {{/each}}
            </ul>
          </div>
        </div>
        {{/each}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with relaxion}}
      <p class="display-3">{{title}}</p>
      <div class="row">
        {{#each cards}}
        <div class="col-5">
          <div class="card text-bg-secondary">
            <div class="card-header">{{header}}</div>
            <div class="card-body">
              <div class="card-title">{{title}}</div>
              <p>{{description}}</p>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with rest}}
      <div class="row">
        {{#each cards}}
        <div class="col-6">
          <div class="card text-center">
            <div class="card-header">{{header}}</div>
            <div class="card-body">
              <div class="card-title">{{title}}</div>
              <p>{{description}}</p>
              <a class="btn btn-primary">{{button}}</a>
            </div>
            <div class="card-footer">{{footer}}</div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with proposition}}
      <p class="display-2">{{title}}</p>
      <div class="row">
        {{#each card}}
        <div class="col-5">
          <div class="card text-bg-warning">
            <div class="card-header">{{header}}</div>
            <div class="card-body">
              <div class="card-title">{{title}}</div>
              <p>{{description}}</p>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/with}}
      <hr>
    </section>

    <section>
      {{#with footer}}
      <h5>{{title}}</h5>
      <div class="row">

        <div class="col-3">
          {{#each navigation}}
          <a class="btn btn-link" href={{href}}>{{text}}</a>
          <br>
          <br>
          {{/each}}
        </div>

        <div class="col-3">
          {{#each socialMedia}}
          <a class="btn btn-outline-dark" href={{link}}>{{name}}</a>
          <br>
          <br>
          {{/each}}
        </div>

        <div class="col-3">
          {{#each partners}}
          <a class="btn btn-outline-info" href={{link}}>{{name}}</a>
          <br>
          <br>
          {{/each}}
        </div>
      </div>
      {{/with}}
      <hr>
    </section>

  </main>
</div>
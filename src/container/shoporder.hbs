<div>
    <main>

        <section class="mx-3 my-5">
            <h2>{{header}}</h2>
        </section>

        <section class="mx-3 my-5">
            <div class="row">
                <div class="col-8">
                    {{#with contacts_data}}
                    <h5>{{title}}</h5>
                    {{#each buttons}}
                    {{#if isPrimary}}
                    <button class="btn btn-warning">{{text}}</button>
                    {{/if}}
                    {{#if isOutline}}
                    <button class="btn btn-outline-warning">{{text}}</button>
                    {{/if}}
                    {{/each}}
                    <div class="row my-3">
                        {{#with form}}
                        {{#each input}}
                        <div class="col-6">
                            <div class="my-3">
                                <label class="form-label"><b>{{label}}</b></label>
                                <input class="form-control" placeholder={{placeholder}}>
                                <div class="form-text">{{caption}}</div>
                            </div>
                        </div>
                        {{/each}}
                        <hr>
                        {{#each switch}}
                        <div class="form-check form-switch py-2">
                            <input class="form-check-input" type="checkbox" {{#if isActive}}checked{{/if}}>
                            <label class="form-check-label">{{text}}</label>
                        </div>
                        {{/each}}
                        <hr>
                        {{/with}}
                    </div>
                    {{/with}}
                    {{#with delivery}}
                    <h2>{{title}}</h2>
                    <div class="card my-3 p-1">
                        <div class="row">
                            {{#each select}}
                            <div class="col-6">
                                {{label}}
                                <select class="form-select">
                                    <option selected>{{placeholder}}</option>
                                    {{#each options}}
                                    <option value={{value}}>{{text}}</option>
                                    {{/each}}
                                </select>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    {{#each delivery_type}}
                    <div class="form-check my-1">
                        <input class="form-check-input" type="radio" name="delivery_type">
                        <label class="form-check-label" for="delivery_type">
                            {{type}}
                        </label>
                    </div>
                    {{/each}}
                    <hr>
                    <div class="my-3">
                        {{#with payment}}
                        <h2>{{title}}</h2>
                        {{#each list}}
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="payment">
                            <label class="form-check-label" for="payment">
                                {{type}}
                            </label>
                            <br>
                            {{description}}
                        </div>
                        {{/each}}
                        {{/with}}
                        <hr>
                    </div>
                    <div class="my-3">
                        {{#with contact}}
                        <div class="row">
                            <h2>{{title}}</h2>
                            {{#with surname}}
                            <div class="col-6">
                                <div class="my-3">
                                    <label class="form-label"><b>{{label}}</b></label>
                                    <input class="form-control" placeholder={{placeholder}}>
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            </div>
                            {{/with}}
                            {{#with name}}
                            <div class="col-6">
                                <div class="my-3">
                                    <label class="form-label"><b>{{label}}</b></label>
                                    <input class="form-control" placeholder={{placeholder}}>
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            </div>
                            {{/with}}
                            {{#with phone}}
                            <div class="col-6">
                                <div class="my-3">
                                    <label class="form-label"><b>{{label}}</b></label>
                                    <input class="form-control" placeholder={{placeholder}}>
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            </div>
                            {{/with}}
                            {{#with email}}
                            <div class="col-6">
                                <div class="my-3">
                                    <label class="form-label"><b>{{label}}</b></label>
                                    <input class="form-control" placeholder={{placeholder}}>
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            </div>
                            {{/with}}
                        </div>
                        <hr>
                        {{/with}}
                    </div>
                    {{#with coment}}
                    {{label}}
                    <div class="input-group mb-3">
                        <input type="text" class="form-control" placeholder={{placeholder}}>
                    </div>
                    <hr>
                    {{/with}}
                    {{/with}}
                    {{#with coupon}}
                    <h2>{{title}}</h2>
                    {{#with select.availabel_coupon}}
                    {{label}}
                    <select class="form-select form-select-lg my-2">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value={{value}}>{{text}}</option>
                        {{/each}}
                    </select>
                    {{/with}}
                    {{#with select.friend_coupon}}
                    {{label}}
                    <select class="form-select form-select-sm my-2">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value={{value}}>{{text}}</option>
                        {{/each}}
                    </select>
                    {{/with}}
                    {{#with add.friend}}
                    <div class="mb-3">
                        <label class="form-label">{{label}}</label>
                        <div class="input-group input-group-lg">
                            <input type="text" class="form-control" placeholder={{placeholder}}>
                        </div>
                        <div class="form-text" id="basic-addon4">{{caption}}</div>
                    </div>
                    {{/with}}
                    {{#with add.colleague}}
                    <div class="mb-3">
                        <label class="form-label">{{label}}</label>
                        <div class="input-group input-group-sm">
                            <input type="text" class="form-control" placeholder={{placeholder}}>
                        </div>
                        <div class="form-text" id="basic-addon4">{{caption}}</div>
                    </div>
                    {{/with}}
                    <hr>
                    {{/with}}
                    <div>
                        {{#with quiz}}
                        <h2>{{title}}</h2>
                        <p>{{preference.title}}</p>
                        {{#each preference.options}}
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="options">
                            <label class="form-check-label" for="options">
                                {{this}}
                            </label>
                        </div>
                        {{/each}}
                        <div class="my-3">
                            <label class="form-label">{{your_expirience.label}}</label>
                            <div class="input-group">
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        {{#with purchase_frequency}}
                        {{label}}
                        <select class="form-select my-3">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                            <option value={{value}}>{{text}}</option>
                            {{/each}}
                        </select>
                        {{/with}}
                        {{#with factors}}
                        {{title}}
                        {{#each options}}
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox">
                            <label class="form-check-label">
                                {{this}}
                            </label>
                        </div>
                        {{/each}}
                        {{/with}}
                        {{/with}}
                    </div>
                </div>
                <div class="col-4">
                    <div class="card my-1">
                        <div class="card-body py-1">
                            {{#each action_block}}
                            {{#if isSuccess}}
                            <button class="btn btn-success my-1">{{text}}</button>
                            <br>
                            {{/if}}
                            {{#if isDanger}}
                            <button class="btn btn-danger my-1">{{text}}</button>
                            <br>
                            {{/if}}{{#if isInfo}}
                            <button class="btn btn-info my-1">{{text}}</button>
                            <br>
                            {{/if}}
                            {{/each}}
                        </div>
                    </div>
                    <div>
                        {{#with total}}
                        <div class="card my-1">
                            <div class="card-title m-1">
                                <span class="display-6"><b>{{title}}:</b></span>
                            </div>
                            <div class="card-body">
                                {{#each item_list}}
                                {{name}}: {{price}}<br>
                                {{/each}}
                                <hr>
                                {{#each additional_info}}
                                {{text}}: {{value}}<br>
                                {{/each}}
                                <hr>
                                {{#with total_price}}
                                {{text}}: {{value}}<br>
                                {{/with}}
                                <hr>
                                {{#each button}}
                                {{#if isPrimary}}
                                <button class="btn btn-primary my-1">{{text}}</button>
                                {{/if}}
                                {{#if isOutline}}
                                <button class="btn btn-outline-primary my-1">{{text}}</button>
                                {{/if}}
                                {{/each}}
                            </div>
                        </div>
                        {{/with}}
                    </div>
                </div>
            </div>
        </section>
    </main>
</div>